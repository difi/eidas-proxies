{
  "defaultAttributes": [
    {
      "name": "PersonIdentifier",
      "required": true
    },
    {
      "name": "DateOfBirth",
      "required": true
    },
    {
      "name": "FamilyName",
      "required": true
    },
    {
      "name": "FirstName",
      "required": true
    }
  ],
  "countries": [
<% @available_countries.sort.each_with_index do |(index, country),i| -%>
    {
      "countryCode": "<%= country['countryCode'] %>",
      "countryName": "<%= country['countryName'] %>",
      "attributes": [<%= country['attributes'] %>],
      "pidAttributeName": <% if (country['pidAttributeName'] == nil) %>null<% else %>"<%= country['pidAttributeName'] %>"<% end %>,
      "pidType": <% if (country['pidType'] == nil) %>null<% else %>"<%= country['pidType'] %>"<% end %>,
      "regex": <% if (country['regex'] == nil) %>null<% else %>"<%= country['regex'] %>"<% end %>
    }<% if (i < @available_countries.length-1) -%>,<% end %>
<% end -%>
  ]
}
